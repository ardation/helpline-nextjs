        "@svgr/webpack": "^5.5.0",
        "@testing-library/jest-dom": "^5.11.6",
        "@testing-library/react": "^11.2.2",
        "@types/css-mediaquery": "^0.1.0",
        "@types/faker": "^5.1.4",
        "@types/google.analytics": "0.0.40",
        "@types/jest": "^26.0.15",
        "@types/lodash": "^4.14.171",
        "@types/moment-timezone": "^0.5.30",
        "@types/node": "^14.14.9",
