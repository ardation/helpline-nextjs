        "react-copy-to-clipboard": "^5.0.4",
        "react-div-100vh": "^0.6.0",
        "react-dom": "17.0.2",
        "react-ga": "^3.3.0",
        "react-google-recaptcha": "^2.1.0",
        "react-gtm-module": "^2.0.11",
