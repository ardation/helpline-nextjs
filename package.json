        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.24.0",
        "fetch-mock": "^9.11.0",
        "husky": "^4.3.0",
        "jest": "^25.5.4",
        "jest-fetch-mock": "^3.0.3",
        "jest-raw-loader": "^1.0.1",
