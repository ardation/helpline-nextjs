        "moment-timezone": "^0.5.32",
        "next": "^11.1.0",
        "next-seo": "^4.26.0",
        "react": "17.0.2",
        "react-copy-to-clipboard": "^5.0.2",
        "react-div-100vh": "^0.3.8",
        "react-dom": "17.0.2",
        "react-ga": "^3.3.0",
        "react-google-recaptcha": "^2.1.0",
        "react-gtm-module": "^2.0.11",
        "react-hotjar": "^2.2.1",
        "react-markdown": "^4.3.1",
