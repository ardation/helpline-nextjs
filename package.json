{
    "name": "helpline-web",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npx webpack --config webpack.config.js && next dev",
        "build": "npx webpack --config webpack.config.js && next build",
        "postbuild": "next-sitemap",
        "start": "next start",
        "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
        "lint:ci": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
        "storybook": "start-storybook -p 6006 -c .storybook -s ./public",
        "storybook-debug-webpack": "storybook --debug-webpack",
        "build-storybook": "build-storybook -c .storybook -s ./public",
        "test": "jest --silent",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "chromatic": "chromatic",
        "codegen": "apollo client:codegen --target typescript --outputFlat  --no-addTypename types"
    },
    "dependencies": {
        "@fontsource/source-sans-pro": "^4.5.0",
        "@fontsource/source-serif-pro": "^4.5.0",
        "@formium/client": "^0.1.3",
